{
    "name": "syntatis/phpstan-psr-11",
    "description": "PHPStan dynamic return type extension for PSR-11 ContainerInterface",
    "type": "phpstan-extension",
    "keywords": [
        "psr-11",
        "service-provider",
        "phpstan",
        "static"
    ],
    "conflict": {
        "bnf/phpstan-psr-container": "*"
    },
    "authors": [
        {
            "name": "Benjamin Franzke",
            "email": "benjaminfranzke@gmail.com",
            "role": "Developer",
            "homepage": "https://bnfr.net"
        },
        {
            "name": "Thoriq Firdaus",
            "role": "Developer"
        }
    ],
    "license": "GPL-3.0",
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "require": {
        "php": ">=7.4",
        "psr/container": "^2.0",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Syntatis\\PHPStan\\Psr11\\": "src/"
        }
    }
}
